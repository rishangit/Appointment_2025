{
  "name": "appointment-management-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@reduxjs/toolkit": "^2.8.2",
    "ag-grid-community": "^34.1.1",
    "ag-grid-react": "^34.1.1",
    "axios": "^1.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.20.1"
  },
  "scripts": {
    "start": "npx vite",
    "dev": "concurrently \"npm run start\" \"npm run sass:watch\"",
    "build": "npm run sass:compile && npx vite build",
    "preview": "npx vite preview",
    "sass:watch": "sass --watch src/styles/index.scss src/styles/index.css --style expanded",
    "sass:compile": "sass src/styles/index.scss src/styles/index.css --style compressed",
    "sass:dev": "sass --watch src/styles/index.scss src/styles/index.css --style expanded --source-map"
  },
  "devDependencies": {
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^9.2.1",
    "sass": "^1.90.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-sass": "^0.1.0"
  }
}
